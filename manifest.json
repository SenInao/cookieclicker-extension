{
  "manifest_version": 3,
  "name": "Cookie clicker helper",
  "version": "1.0",
  "permissions": ["scripting", "tabs", "notifications"],
  "host_permissions": ["https://orteil.dashnet.org/cookieclicker/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "cookieclicker.html"
  }
}
